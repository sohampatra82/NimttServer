    <script>
        let isEditing = false;
        let paymentHistoryChanged = false; // Flag to track payment history changes
        let originalPaymentDates = {}; // Store original payment dates
        let originalPaymentModes = {}; // Store original payment modes

        const showModal = (title, message) => {
            const modal = document.getElementById('modal');
            document.getElementById('modal-title').textContent = title;
            document.getElementById('modal-message').textContent = message;
            modal.style.display = 'flex';
        };

        document.getElementById('modal-close').addEventListener('click', () => {
            document.getElementById('modal').style.display = 'none';
        });

        const fetchStudentData = async (Stdid) => {
            if (Stdid === '') {
                document.getElementById('user-details').innerHTML = '';
                document.getElementById('user-details').style.display = 'none';
                document.getElementById('update-data').style.display = 'none';
                document.getElementById('save-data').style.display = 'none';
                isEditing = false;
                paymentHistoryChanged = false;
                originalPaymentDates = {};
                originalPaymentModes = {};
                return;
            }

            try {
                const response = await fetch('/admin-fetch-data', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({ Stdid })
                });

                const data = await response.json();

                if (response.ok) {
                    const formatDate = (isoDate) => {
                        const date = new Date(isoDate);
                        const day = String(date.getDate()).padStart(2, '0');
                        const month = String(date.getMonth() + 1).padStart(2, '0');
                        const year = date.getFullYear();
                        return `${day}-${month}-${year}`;
                    };

                    // Store original payment dates and modes
                    originalPaymentDates = {};
                    originalPaymentModes = {};
                    data.PaymentHistory.forEach(payment => {
                        originalPaymentDates[payment.paymentNumber] = payment.paymentDate;
                        originalPaymentModes[payment.paymentNumber] = payment.paymentMode || 'N/A';
                    });

                    const paymentHistoryHTML = data.PaymentHistory.map(payment => `
                        <li data-payment-number="${payment.paymentNumber}">
                            Payment Number ${payment.paymentNumber} : ₹${payment.amountPaid} via ${payment.paymentMode || 'N/A'} on ${formatDate(payment.paymentDate)}
                        </li>
                    `).join('');

                    document.getElementById('user-details').innerHTML = `
                        <table class="excel-table">
                            <tr>
                                <th>Field</th>
                                <th>Details</th>
                            </tr>
                            <tr>
                                <td>Student Name</td>
                                <td data-field="Stdname">${data.Stdname}</td>
                            </tr>
                            <tr>
                                <td>Admitted By</td>
                                <td data-field="StdAdmittedBy">${data.StdAdmittedBy || "N/A"}</td>
                            </tr>
                            <tr>
                                <td>University Name</td>
                                <td data-field="stdUniversityName">${data.stdUniversityName || "N/A"}</td>
                            </tr>
                            <tr>
                                <td>Course</td>
                                <td data-field="Stdcourse">${data.Stdcourse || "N/A"}</td>
                            </tr>
                            <tr>
                                <td>Course Mode</td>
                                <td data-field="stdmode">${data.stdmode || "N/A"}</td>
                            </tr>
                            <tr>
                                <td>Specialization</td>
                                <td data-field="stdDateOfAdmission">${data.stdDateOfAdmission || "N/A"}</td>
                            </tr>
                            <tr>
                                <td>Exam Session</td>
                                <td data-field="StdExamSession">${data.StdExamSession || "N/A"}</td>
                            </tr>
                            <tr>
                                <td>Full Course Fee</td>
                                <td data-field="Stdfullfees">₹${data.Stdfullfees}</td>
                            </tr>
                            <tr>
                                <td>Total Paid Fee</td>
                                <td data-field="Stdpaidfee">₹${data.Stdpaidfee}</td>
                            </tr>
                            <tr>
                                <td>Due Fee</td>
                                <td data-field="Stdduefee">₹${data.Stdduefee}</td>
                            </tr>
                            <tr>
                                <td>Fees Breakout</td>
                                <td data-field="stdtotalbreakdetails">${data.stdtotalbreakdetails}</td>
                            </tr>
                        </table>
                        <div class="payment-history">
                            <h3>
                                <svg width="18" height="18" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8c-1.657 0-3 .895-3 2s1.343 2 3 2 3 .895 3 2-1.343 2-3 2m0-8c1.11 0 2.08.402 2.599 1M12 8V7m0 1v8m0 0v1m0-1c-1.11 0-2.08-.402-2.599-1M21 12a9 9 0 11-18 0 9 9 0 0118 0z"/>
                                </svg>
                                Payment History
                            </h3>
                            <ul>${paymentHistoryHTML || '<li>No payments recorded yet</li>'}</ul>
                            <button class="add-payment-btn" id="add-payment" style="display: none; margin-top: 12px;">
                                <svg width="16" height="16" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 4v16m8-8H4" />
                                </svg>
                                Add Payment
                            </button>
                        </div>
                    `;
                    document.getElementById('user-details').style.display = 'block';
